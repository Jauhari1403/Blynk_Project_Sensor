      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "from tensorflow.keras.models import Sequential\n",
        "from tensorflow.keras.layers import Embedding, SimpleRNN, Dense\n",
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "from tensorflow.keras.preprocessing.sequence import pad_sequences\n",
        "\n",
        "#Data komentar pada hasil pertandingan Liverpool dengan ManCity dan Label\n",
        "comments = [\"Berikan Salah apapun yang dia inginkan\", \"Slot menunjukkan kelasnya\", \"ManCity bermain sangat buruk\", \"Pep harus segera dipecat\", \"Kita (Liverpool) adalah tim terbaik di dunia\"]\n",
        "labels =[1, 1, 0, 0, 1]\n",
        "\n",
        "#tokenisasi teks\n",
        "tokenizer = Tokenizer()\n",
        "tokenizer.fit_on_texts(comments)\n",
        "sequences = tokenizer.texts_to_sequences(comments)\n",
        "word_index = tokenizer.word_index\n",
        "print(\"word index:\", word_index)\n",
        "\n",
        "# Padding sequences\n",
        "maxlen = 4\n",
        "x = pad_sequences(sequences, maxlen=maxlen)\n",
        "y = np.array(labels)\n",
        "\n",
        "#Membangun model RNN\n",
        "model = Sequential([\n",
        "    Embedding(input_dim=len(word_index) + 1, output_dim=8, input_length=maxlen),\n",
        "    SimpleRNN(50, activation='relu'),\n",
        "    Dense(1, activation='sigmoid')\n",
        "])\n",
        "\n",
        "# kompilasi model\n",
        "model.compile(optimizer='adam', loss='binary_crossentropy', metrics=['accuracy'])\n",
        "\n",
        "# LAtih model\n",
        "model.fit(x, y, epochs=10, batch_size=2, verbose=1)\n",
        "\n",
        "#Prediksi komentar pada hasil pertandingan Liverpool dengan ManCity\n",
        "new_comments = [\"Slot menunjukkan kelasnya\", \"Pep harus segera dipecat\"]\n",
        "new_sequences = tokenizer.texts_to_sequences(new_comments)\n",
        "new_X = pad_sequences(new_sequences, maxlen=maxlen)\n",
        "predictions = model.predict(new_X)\n",
        "\n",
        "for comment, pred in zip(new_comments, predictions):\n",
        "  print(f\"komentar: {comment} -> Sentimen: {'Positif' if pred > 0.5 else 'Negatif'}\")"
      ],
